@model ELCAPITAL.Models.Solicitud

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TipoSolicitud" class="control-label"></label>
                <input asp-for="TipoSolicitud" class="form-control" />
                <span asp-validation-for="TipoSolicitud" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input class="form-control" type="text" value="@Model.FechaSolicitud" aria-label="Fecha Solicitud" readonly>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

