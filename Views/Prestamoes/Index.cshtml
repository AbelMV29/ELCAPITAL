@model List<ELCAPITAL.Models.Prestamo>

@{
    ViewData["Title"] = "Index";
}

<h1>MIS PRESTAMOS</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                Id Producto
            </th>
            <th>
                Es Prendario
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdProducto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EsPrendario)
            </td>
        </tr>
}
    </tbody>
</table>
