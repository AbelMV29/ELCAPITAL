@model List<ELCAPITAL.Models.Paquete>

@{
    ViewData["Title"] = "Index";
}

<h1>MIS PAQUETES</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Id Producto
            </th>
            <th>
                Es Crediticio
            </th>
            <th>
                Detalles
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdProducto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EsCrediticio)
            </td>
                 
                <td>
                    <a asp-action="Details">Detalles</a>
                </td>
        </tr>
}
    </tbody>
</table>
